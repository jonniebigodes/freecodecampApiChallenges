{
  "name": "freecodecampapichallenges",
  "version": "0.0.2",
  "description": "freecodecamp api challenges app",
  "main": "server.js",
  "scripts": {
    "testclient": "jest .test.js --config=jest.config.json",
    "testserver": "mocha --compilers js:babel-core/register --require babel-polyfill \"__tests__/server/**/*.js\" --exit --timeout 30000",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "start": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack -d --watch",
    "showerrors": "webpack --progress --color --watch --display-error-details",
    "showinfo": "webpack --display-modules --display-reasons",
    "showinfofile": "webpack --json>stats.json",
    "devserver": "nodemon server.js --exec babel-node",
    "devclient": "webpack-dev-server --mode development --open",
    "builddev": "webpack --config webpack.config.js --progress --colors --mode development",
    "buildserver": "babel server.js -o ./lib/server.bundle.js --presets minify",
    "buildlogger": "babel logger.js -o ./lib/logger.js --presets minify",
    "buildmodels": "babel ./src/models/ --out-dir ./lib --presets minify",
    "buildexercises": "babel ./src/Challenges/ExerciseTracker/Exercise.js -o ./lib/Exercise.js --presets minify",
    "buildheader": "babel ./src/Challenges/headerChallenge/headerParser.js -o ./lib/headerParser.js --presets minify",
    "buildimages": "babel ./src/Challenges/imageSearch/ImgSearcher.js -o ./lib/ImgSearcher.js --presets minify",
    "buildtime": "babel ./src/Challenges/timeCheckChallenge/timeChecker.js -o ./lib/timeChecker.js --presets minify",
    "buildshorter": "babel ./src/Challenges/UrlshortChallenge/UrlShortener.js -o ./lib/UrlShortener.js --presets minify",
    "build": "npm run buildserver && npm run buildlogger && npm run buildmodels && npm run buildexercises && npm run buildheader && npm run buildimages && npm run buildtime && npm run buildtime",
    "postinstall": "npm run build && webpack --config webpack.prod.config.js --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonniebigodes/freecodecampApiChallenges.git"
  },
  "keywords": [
    "heroku",
    "api",
    "freecodecamp",
    "challenges"
  ],
  "author": "jonniebigodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonniebigodes/freecodecampApiChallenges/issues"
  },
  "engines": {
    "node": "8.11.3"
  },
  "homepage": "https://github.com/jonniebigodes/freecodecampApiChallenges#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.22.2",
    "mongodb": "^3.0.10",
    "mongoose": "^5.1.5",
    "multer": "^1.3.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.0.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-merge": "^4.1.3",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-jest": "^23.2.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.4.1",
    "superagent": "^3.8.3",
    "webpack-dev-server": "^3.1.4"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/enzymeconfig.js"
    ]
  }
}
